{
  "manifest_version" : 1,
  
  "name" : "youtube-iframe",
  "description" : "DEMO",
  "version" : "0.0.1",
  
  "browser_action" : {
    "default_title" : "youtube-iframe"
  },
  "content_scripts" : [
    {
      "matches" : ["*://*.youtube.com/*"],
      "js" : ["index.js"]
    }
  ],
  "permissions" : ["activeTab"]
}
